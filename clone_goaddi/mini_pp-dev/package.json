{
  "name": "gopaddi",
  "version": "0.2.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "server": "turbo start",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:paddi": "pnpm --filter go-paddi run dev",
    "start:paddi": "pnpm --filter go-paddi start",
    "build:paddi": "pnpm --filter go-paddi run build"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "react-responsive": "^10.0.0"
  }
}
